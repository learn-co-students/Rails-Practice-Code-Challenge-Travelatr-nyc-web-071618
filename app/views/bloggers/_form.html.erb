<%= form_for @blogger do |f| %>
<ul>
<% if @blogger.errors.any? %>
  <% @blogger.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
    <% end  %>
  <% end %>
</ul>
<br>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :bio %>
<%= f.text_field :bio %>
<br>
<%= f.label :age %>
<%= f.number_field :age %>
<br>
<%= f.submit %>
<% end %>
